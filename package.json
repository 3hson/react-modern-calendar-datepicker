{
  "name": "3hson-react-modern-calendar-datepicker",
  "version": "3.2.4",
  "description": "A modern, beautiful, customizable date picker for React",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "lint": "concurrently \"eslint src\" \"prettier --check \"./src/**/*.css\"\" ",
    "lint:write": "prettier --write \"./src/**/*.js\"  \"./src/**/*.css\"",
    "test": "jest ./test --coverage",
    "test:watch": "jest ./test --watch",
    "clean": "rimraf lib",
    "build": "cross-env NODE_ENV=production rollup -c --bundleConfigAsCjs",
    "prepare": "husky install",
    "size": "npm run build && size-limit",
    "checkAll": "npm run size && npm run lint && npm run test",
    "contributors:generate": "npm all-contributors generate"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "prepare": "husky install",
  "author": "ehsan khakian",
  "homepage": "https://kiarash-z.github.io/react-modern-calendar-datepicker",
  "repository": {
    "type": "git",
    "url": "https://github.com/3hson/react-modern-calendar-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/3hson/react-modern-calendar-datepicker/issues"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "modern",
    "calendar",
    "datepicker",
    "datepicker range",
    "datepicker component",
    "beautiful",
    "animated",
    "javascript",
    "persian"
  ],
  "size-limit": [
    {
      "path": "./lib/index.js",
      "limit": "11 KB"
    }
  ],
  "dependencies": {
    "jalaali-js": "^1.2.7"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@size-limit/preset-small-lib": "^11.1.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "all-contributors-cli": "^6.26.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "image-webpack-loader": "^8.1.0",
    "jest": "^29.7.0",
    "lint-staged": ">=15",
    "prettier": "^3.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
